  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  spdchave,'
      '  spdexercicio,'
      '  spddatainicial,'
      '  spddatafinal,'
      '  spddatabalanco,'
      '  pcccodigo,'
      '  spdativo,'
      '  IF(spdativo = '#39'1'#39', '#39'A'#39', '#39' '#39') AS ativo,'
      '  spdregistro,'
      '  spdenvio,'
      '  spdpasta,'
      ' IF( (SELECT'
      
        '      COUNT(ivtchave) FROM ivt WHERE ivt.spdchave=spd.spdchave )' +
        '>0,'#39'Sim'#39','#39'N'#227'o'#39') ivtgerado,'
      ' IF( (SELECT'
      
        '      COUNT(ivdchave) FROM ivd WHERE ivd.spdchave=spd.spdchave )' +
        '>0,'#39'Sim'#39','#39'N'#227'o'#39') ivdgerado'
      'FROM spd')
    Left = 552
    Top = 196
