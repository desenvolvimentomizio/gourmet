    property itmbasefcpicmst :Currency read Getitmbasefcpicmst write Setitmbasefcpicmst;
    [Campo('itmpercfcpicmst')]
    property itmpercfcpicmst :Currency read Getitmpercfcpicmst write Setitmpercfcpicmst;
    [Campo('itmvalofcpicmst')]
    property itmvalofcpicmst :Currency read Getitmvalofcpicmst write Setitmvalofcpicmst;

    [Campo('COUNT'), Ignore]
    property COUNT : Integer read FCOUNT write SetCOUNT;
end;

