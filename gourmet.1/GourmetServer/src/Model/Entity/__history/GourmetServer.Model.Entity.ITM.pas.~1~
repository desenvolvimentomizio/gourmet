procedure Titm.Limpar;
begin
  Self.itmchave := 0;
  Self.meschave := 0;
  Self.itmitem := 0;
  Self.procodigo := 0;
  Self.cstcodigo := '';
  Self.itmdesccomple := '';
  Self.itmquantidade := 0;
  Self.itmvalor := 0;
  Self.itmdesconto := 0;
  Self.toecodigo := 0;
  Self.cfocfop := '';
  Self.icmcodigo := '';
  Self.itmbicm := 0;
  Self.itmaliqicm := '';
  Self.itmicm := 0;
  Self.itmbicms := 0;
  Self.itmaliqicms := 0;
  Self.itmicms := 0;
  Self.csicodigo := '';
  Self.ceicodigo := '';
  Self.itmbipi := 0;
  Self.itmaliqipi := 0;
  Self.itmipi := 0;
  Self.cspcodigo := '';
  Self.itmbpis := 0;
  Self.itmaliqpis := 0;
  Self.itmquantpis := 0;
  Self.itmaliqpisvalor := 0;
  Self.itmpis := 0;
  Self.csfcodigo := '';
  Self.itmbcofins := 0;
  Self.itmaliqcofins := 0;
  Self.itmquantcofins := 0;
  Self.itmaliqcofinsvalor := 0;
  Self.itmcofins := 0;
  Self.pcccodigo := '';
  Self.itmtotal := 0;
  Self.unicodigo := 0;
  Self.itmicmsvalor := 0;
  Self.puncodigo := 0;
  Self.procodigoori := '';
  Self.pronomeori := '';
  Self.progtin := '';
  Self.itmcontendo := 0;
  Self.cfocfopdestinacao := '';
  Self.unicodigobase := 0;
  Self.itmcusto := 0;
  Self.itmtipodesc := 0;
  Self.itmfrete := 0;
  Self.itmseguro := 0;
  Self.itmoutras := 0;
  Self.itmsped := 0;
  Self.itmpercdesc := 0;
  Self.itmvlribpt := 0;
  Self.itmproservico := '';
  Self.itminfadprod := '';
  Self.itmquanticontada := 0;
  Self.flacodigo := 0;
  Self.tdfcodigo := '';
  Self.itmtipocomissao := 0;
  Self.itmoutroscustos := 0;
  Self.itochave := 0;
  Self.itmpercreducaobaseicm := 0;
  Self.pmpchave := 0;
  Self.itmtempoproducao := 0;
  Self.itmtempoentrega := 0;
  Self.itmitempedido := '';
  Self.itmcest := '';
  Self.itmmva := 0;
  Self.pcocodigo := 0;
  Self.itmpercentualcombo := 0;
  Self.clbatendente := 0;
  Self.clbentregador := 0;
  Self.oricodigo := 0;
  Self.itmbasefcpicm := 0;
  Self.itmpercfcpicm := 0;
  Self.itmvalofcpicm := 0;
  Self.itmbasefcpicmst := 0;
  Self.itmpercfcpicmst := 0;
  Self.itmvalofcpicmst := 0;

end;

procedure Titm.SetCOUNT(const Value: Integer);
begin
  FCOUNT := Value;
end;
function Titm.ToJson(pFields : String = '*') : TJSonObject;
begin
  pFields := ' '+ StringReplace(pFields, ',', ' ', [rfReplaceAll]);
  result := TJSonObject.Create;
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmchave ')) then
    result.addPair('itmchave', IntToStr(Self.itmchave));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' meschave ')) then
    result.addPair('meschave', IntToStr(Self.meschave));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmitem ')) then
    result.addPair('itmitem', IntToStr(Self.itmitem));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' procodigo ')) then
    result.addPair('procodigo', IntToStr(Self.procodigo));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' cstcodigo ')) then
    result.addPair('cstcodigo', Self.cstcodigo);
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmdesccomple ')) then
    result.addPair('itmdesccomple', Self.itmdesccomple);
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmquantidade ')) then
    result.addPair('itmquantidade', FloatToStr(Self.itmquantidade));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmvalor ')) then
    result.addPair('itmvalor', FloatToStr(Self.itmvalor));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmdesconto ')) then
    result.addPair('itmdesconto', FloatToStr(Self.itmdesconto));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' toecodigo ')) then
    result.addPair('toecodigo', IntToStr(Self.toecodigo));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' cfocfop ')) then
    result.addPair('cfocfop', Self.cfocfop);
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' icmcodigo ')) then
    result.addPair('icmcodigo', Self.icmcodigo);
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmbicm ')) then
    result.addPair('itmbicm', FloatToStr(Self.itmbicm));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmaliqicm ')) then
    result.addPair('itmaliqicm', Self.itmaliqicm);
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmicm ')) then
    result.addPair('itmicm', FloatToStr(Self.itmicm));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmbicms ')) then
    result.addPair('itmbicms', FloatToStr(Self.itmbicms));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmaliqicms ')) then
    result.addPair('itmaliqicms', FloatToStr(Self.itmaliqicms));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmicms ')) then
    result.addPair('itmicms', FloatToStr(Self.itmicms));
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' csicodigo ')) then
    result.addPair('csicodigo', Self.csicodigo);
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' ceicodigo ')) then
    result.addPair('ceicodigo', Self.ceicodigo);
  if (pFields.Contains('*') or AnsiLowerCase(pFields).Contains(' itmbipi ')) then
    result.addPair('itmbipi', FloatToStr(Self.itmbipi));
