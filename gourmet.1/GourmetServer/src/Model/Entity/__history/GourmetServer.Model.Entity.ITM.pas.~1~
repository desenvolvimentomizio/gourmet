function Titm.JsonToClass(obj : TJSonObject) : Titm;
begin
  result := Titm.Create;
  if (assigned(obj)) then
  begin
    Try
      Try result.itmchave := StrToInt(obj.get('itmchave').JsonValue.Value); Except End;
      Try result.meschave := StrToInt(obj.get('meschave').JsonValue.Value); Except End;
      Try result.itmitem := StrToInt(obj.get('itmitem').JsonValue.Value); Except End;
      Try result.procodigo := StrToInt(obj.get('procodigo').JsonValue.Value); Except End;
      Try result.cstcodigo := obj.get('cstcodigo').JsonValue.Value; Except End;
      Try result.itmdesccomple := obj.get('itmdesccomple').JsonValue.Value; Except End;
      Try result.itmquantidade := StrToFloat(obj.get('itmquantidade').JsonValue.Value); Except End;
      Try result.itmvalor := StrToFloat(obj.get('itmvalor').JsonValue.Value); Except End;
      Try result.itmdesconto := StrToFloat(obj.get('itmdesconto').JsonValue.Value); Except End;
      Try result.toecodigo := StrToInt(obj.get('toecodigo').JsonValue.Value); Except End;
      Try result.cfocfop := obj.get('cfocfop').JsonValue.Value; Except End;
      Try result.icmcodigo := obj.get('icmcodigo').JsonValue.Value; Except End;
      Try result.itmbicm := StrToFloat(obj.get('itmbicm').JsonValue.Value); Except End;
      Try result.itmaliqicm := obj.get('itmaliqicm').JsonValue.Value; Except End;
      Try result.itmicm := StrToFloat(obj.get('itmicm').JsonValue.Value); Except End;
      Try result.itmbicms := StrToFloat(obj.get('itmbicms').JsonValue.Value); Except End;
      Try result.itmaliqicms := StrToFloat(obj.get('itmaliqicms').JsonValue.Value); Except End;
      Try result.itmicms := StrToFloat(obj.get('itmicms').JsonValue.Value); Except End;
      Try result.csicodigo := obj.get('csicodigo').JsonValue.Value; Except End;
      Try result.ceicodigo := obj.get('ceicodigo').JsonValue.Value; Except End;
      Try result.itmbipi := StrToFloat(obj.get('itmbipi').JsonValue.Value); Except End;
      Try result.itmaliqipi := StrToFloat(obj.get('itmaliqipi').JsonValue.Value); Except End;
      Try result.itmipi := StrToFloat(obj.get('itmipi').JsonValue.Value); Except End;
      Try result.cspcodigo := obj.get('cspcodigo').JsonValue.Value; Except End;
      Try result.itmbpis := StrToFloat(obj.get('itmbpis').JsonValue.Value); Except End;
      Try result.itmaliqpis := StrToFloat(obj.get('itmaliqpis').JsonValue.Value); Except End;
      Try result.itmquantpis := StrToFloat(obj.get('itmquantpis').JsonValue.Value); Except End;
      Try result.itmaliqpisvalor := StrToFloat(obj.get('itmaliqpisvalor').JsonValue.Value); Except End;
      Try result.itmpis := StrToFloat(obj.get('itmpis').JsonValue.Value); Except End;
      Try result.csfcodigo := obj.get('csfcodigo').JsonValue.Value; Except End;
      Try result.itmbcofins := StrToFloat(obj.get('itmbcofins').JsonValue.Value); Except End;
      Try result.itmaliqcofins := StrToFloat(obj.get('itmaliqcofins').JsonValue.Value); Except End;
      Try result.itmquantcofins := StrToFloat(obj.get('itmquantcofins').JsonValue.Value); Except End;
      Try result.itmaliqcofinsvalor := StrToFloat(obj.get('itmaliqcofinsvalor').JsonValue.Value); Except End;
      Try result.itmcofins := StrToFloat(obj.get('itmcofins').JsonValue.Value); Except End;
      Try result.pcccodigo := obj.get('pcccodigo').JsonValue.Value; Except End;
      Try result.itmtotal := StrToFloat(obj.get('itmtotal').JsonValue.Value); Except End;
      Try result.unicodigo := StrToInt(obj.get('unicodigo').JsonValue.Value); Except End;
      Try result.itmicmsvalor := StrToFloat(obj.get('itmicmsvalor').JsonValue.Value); Except End;
      Try result.puncodigo := StrToInt(obj.get('puncodigo').JsonValue.Value); Except End;
      Try result.procodigoori := obj.get('procodigoori').JsonValue.Value; Except End;
      Try result.pronomeori := obj.get('pronomeori').JsonValue.Value; Except End;
      Try result.progtin := obj.get('progtin').JsonValue.Value; Except End;
      Try result.itmcontendo := StrToFloat(obj.get('itmcontendo').JsonValue.Value); Except End;
      Try result.cfocfopdestinacao := obj.get('cfocfopdestinacao').JsonValue.Value; Except End;
      Try result.unicodigobase := StrToInt(obj.get('unicodigobase').JsonValue.Value); Except End;
      Try result.itmcusto := StrToFloat(obj.get('itmcusto').JsonValue.Value); Except End;
      Try result.itmtipodesc := StrToInt(obj.get('itmtipodesc').JsonValue.Value); Except End;
      Try result.itmfrete := StrToFloat(obj.get('itmfrete').JsonValue.Value); Except End;
      Try result.itmseguro := StrToFloat(obj.get('itmseguro').JsonValue.Value); Except End;
      Try result.itmoutras := StrToFloat(obj.get('itmoutras').JsonValue.Value); Except End;
      Try result.itmsped := StrToInt(obj.get('itmsped').JsonValue.Value); Except End;
      Try result.itmpercdesc := StrToFloat(obj.get('itmpercdesc').JsonValue.Value); Except End;
      Try result.itmvlribpt := StrToFloat(obj.get('itmvlribpt').JsonValue.Value); Except End;
      Try result.itmproservico := obj.get('itmproservico').JsonValue.Value; Except End;
      Try result.itminfadprod := obj.get('itminfadprod').JsonValue.Value; Except End;
      Try result.itmquanticontada := StrToFloat(obj.get('itmquanticontada').JsonValue.Value); Except End;
      Try result.flacodigo := StrToInt(obj.get('flacodigo').JsonValue.Value); Except End;
      Try result.tdfcodigo := obj.get('tdfcodigo').JsonValue.Value; Except End;
      Try result.itmtipocomissao := StrToInt(obj.get('itmtipocomissao').JsonValue.Value); Except End;
      Try result.itmoutroscustos := StrToFloat(obj.get('itmoutroscustos').JsonValue.Value); Except End;
      Try result.itochave := StrToInt(obj.get('itochave').JsonValue.Value); Except End;
      Try result.itmpercreducaobaseicm := StrToFloat(obj.get('itmpercreducaobaseicm').JsonValue.Value); Except End;
      Try result.pmpchave := StrToInt(obj.get('pmpchave').JsonValue.Value); Except End;
      Try result.itmtempoproducao := StrToDateTime(obj.get('itmtempoproducao').JsonValue.Value); Except End;
      Try result.itmtempoentrega := StrToDateTime(obj.get('itmtempoentrega').JsonValue.Value); Except End;
      Try result.itmitempedido := obj.get('itmitempedido').JsonValue.Value; Except End;
      Try result.itmcest := obj.get('itmcest').JsonValue.Value; Except End;
      Try result.itmmva := StrToFloat(obj.get('itmmva').JsonValue.Value); Except End;
      Try result.pcocodigo := StrToInt(obj.get('pcocodigo').JsonValue.Value); Except End;
      Try result.itmpercentualcombo := StrToFloat(obj.get('itmpercentualcombo').JsonValue.Value); Except End;
      Try result.clbatendente := StrToInt(obj.get('clbatendente').JsonValue.Value); Except End;
      Try result.clbentregador := StrToInt(obj.get('clbentregador').JsonValue.Value); Except End;
      Try result.oricodigo := StrToInt(obj.get('oricodigo').JsonValue.Value); Except End;
      Try result.itmbasefcpicm := StrToFloat(obj.get('itmbasefcpicm').JsonValue.Value); Except End;
      Try result.itmpercfcpicm := StrToFloat(obj.get('itmpercfcpicm').JsonValue.Value); Except End;
      Try result.itmvalofcpicm := StrToFloat(obj.get('itmvalofcpicm').JsonValue.Value); Except End;
      Try result.itmbasefcpicmst := StrToFloat(obj.get('itmbasefcpicmst').JsonValue.Value); Except End;
      Try result.itmpercfcpicmst := StrToFloat(obj.get('itmpercfcpicmst').JsonValue.Value); Except End;
      Try result.itmvalofcpicmst := StrToFloat(obj.get('itmvalofcpicmst').JsonValue.Value); Except End;

    Except
      on E : Exception do
      begin
        result.Free;
        raise Exception.Create(E.Message);
      end;
    End;
  end;
end;

