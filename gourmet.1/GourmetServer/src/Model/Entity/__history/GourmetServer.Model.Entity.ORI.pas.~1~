unit GourmetServer.Model.Entity.ORI;

interface

{$TYPEINFO ON}

Uses
  SimpleAttributes;

type

  [Tabela('brd')]
  TBRD = Class
  private
    fbrdcodigo: Integer;
    fbrdidentificacao: string;
    fprocodigo: Integer;
    fsipcodigo: Integer;
    procedure setbrdcodigo(const Value: Integer);
    procedure setbrdidentificacao(const Value: String);
    procedure setprocodigo(const Value: Integer);
    procedure setsipcodigo(const Value: Integer);
  public
    constructor create;
    destructor destroy; override;
  published

    [campo('brdcodigo'), PK, AutoInc]
    property brdcodigo: Integer read fbrdcodigo write setbrdcodigo;

    [campo('brdidentificacao')]
    property brdidentificacao: String read fbrdidentificacao write setbrdidentificacao;

    [campo('procodigo')]
    property procodigo: Integer read fprocodigo write setprocodigo;

    [campo('sipcodigo')]
    property sipcodigo: Integer read fsipcodigo write setsipcodigo;

  End;

implementation

{ TBRD }

constructor TBRD.create;
begin

end;

destructor TBRD.destroy;
begin

  inherited;
end;

procedure TBRD.setbrdcodigo(const Value: Integer);
begin
  fbrdcodigo := Value;
end;

procedure TBRD.setbrdidentificacao(const Value: String);
begin
  fbrdidentificacao := Value;
end;

procedure TBRD.setprocodigo(const Value: Integer);
begin
  fprocodigo := Value;
end;

procedure TBRD.setsipcodigo(const Value: Integer);
begin
  fsipcodigo := Value;
end;

end.
