      Except
      End;
      Try
        Result.propercreducaobaseicm := StrToFloat(obj.get('propercreducaobaseicm').JsonValue.Value);
      Except
      End;
      Try
        Result.propisaliquota := StrToFloat(obj.get('propisaliquota').JsonValue.Value);
