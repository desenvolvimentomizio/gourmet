unit GourmetServer.Model.Entity.V_CLI;

interface

{$TYPEINFO ON}

Uses
  SimpleAttributes;

type

  [Tabela('v_cli')]
  TV_CLI = Class
  private
    fetdcodigo: integer;
    fetftelefone: string;
    fetddoc1: string;

    feteemail: string;
    fetfsenha: string;

    procedure setetdcodigo(const Value: integer);
    procedure setetftelefone(const Value: string);
    procedure setetfsenha(const Value: string);
    procedure seteteemail(const Value: string);
    procedure setetddoc1(const Value: string);

  public
    constructor create;
    destructor destroy; override;
  published

    [campo('etdcodigo')]
    property etdcodigo: integer read fetdcodigo write setetdcodigo;

    [campo('etftelefone')]
    property etftelefone: string read fetftelefone write setetftelefone;

    [campo('etddoc1')]
    property etddoc1: string read fetddoc1 write setetddoc1;

    [campo('etfsenha')]
    property etfsenha: string read fetfsenha write setetfsenha;

    [campo('eteemail')]
    property eteemail: string read feteemail write seteteemail;

  End;

implementation

{ TV_CLI }

constructor TV_CLI.create;
begin

end;

destructor TV_CLI.destroy;
begin
  inherited;
end;

procedure TV_CLI.setetdcodigo(const Value: integer);
begin
  fetdcodigo := Value;
end;

procedure TV_CLI.setetddoc1(const Value: string);
begin
    fetddoc1 := Value;
end;

procedure TV_CLI.seteteemail(const Value: string);
begin
  feteemail := Value;
end;

procedure TV_CLI.setetfsenha(const Value: string);
begin
  fetfsenha := Value;
end;

procedure TV_CLI.setetftelefone(const Value: string);
begin
  fetftelefone := Value;
end;

end.
