unit GourmetServer.Controller.ORI;

interface

Uses
  System.Json,
  System.SysUtils,
  idHashMessageDigest,
  GourmetServer.Model.DAOGeneric,
  GourmetServer.Model.Entity.ORI;

function BuscaEtdCodigoOriCodigo(vOriCodigo:Integer): Integer;

type
  TAPIError = class
  private
    Ferror: string;
  public
    property error: string read Ferror write Ferror;
  end;

implementation

function BuscaEtdCodigoOriCodigo(vOriCodigo:Integer): Integer;
var
  FDAO: iDAOGeneric<TORI>;
  vletdcodigo: Integer;
begin
  vletdcodigo := '';

  FDAO := TDAOGeneric<TORI>.New;

  FDAO.DAO.SQL.where('oricodigo=' +inttostr(vOriCodigo)).&End.Find;

  vletdcodigo := FDAO.DataSet.FieldByName('etdcodigo').asInteger;

  result := vletdcodigo;
end;



