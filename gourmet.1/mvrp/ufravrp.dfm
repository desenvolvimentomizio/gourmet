  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  vrp.vrpcodigo,'
      '  vrp.procodigo,'
      '  vrp.vrpreferencia,'
      '  vrp.vrpbarra,'
      '  via.viaatributos,'
      '  vrp.vrpsaldo'
      'FROM vrp'
      '  JOIN (SELECT'
      '      via.vrpcodigo,'
      
        '      CONVERT(GROUP_CONCAT(iat.iatidentificacao ORDER BY via.via' +
        'codigo SEPARATOR '#39' - '#39'), char(40) charset latin1) AS viaatributo' +
        's'
      '    FROM via'
      '      JOIN iat'
      '        ON via.iatcodigo = iat.iatcodigo'
      '    GROUP BY via.vrpcodigo) via'
      '    ON via.vrpcodigo = vrp.vrpcodigo'
      'WHERE vrp.procodigo = :procodigo')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'procodigo'
        Value = nil
      end>
