  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  sbr.sbrcodigo,'
      '  sbr.sbridentificacao,'
      '  pro.pronome,'
      '  sen.senidentificacao,'
      '  sbr.sbrordem,'
      '  grp.grpidentificacao,'
      '  sip.sipcodigo'
      'FROM sbr,'
      '     pro,'
      '     sen,'
      '     grp,'
      '     sip'
      'WHERE sbr.procodigo = pro.procodigo'
      'AND sbr.sbrcobraadicional = sen.sencodigo'
      'AND pro.grpcodigo = grp.grpcodigo'
      'AND sbr.sipcodigo = sip.sipcodigo')
