  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT DISTINCT etd.etdcodigo'
      '              , etd.etdidentificacao'
      '              , etd.etdapelido'
      '              , tpe.tpeidentificacao'
      '              , tseidentificacao'
      '              , etd.tsecodigo'
      '              , tvi.tvicodigo'
      '              , etd.tpecodigo'
      '              , etd.etddoc1'
      '              , edr.edrinscest'
      '              , etd.etddatacad'
      '              , etd.etddataalt'
      '              , etd.etdativo'
      '              '
      '              , etd.etddescsituacao'
      '              , etd.etdsped'
      '              , etd.etdobs'
      '              , 0 etllimitetotal'
      '              , 0 etldisponivel'
      '              , 0 rfivencido'
      '              , CONCAT(edr.edrrua, '#39','#39', edr.edrnumero) AS edrrua'
      '              , etf.etftelefone'
      '--              , grt.grtidentificacao,'
      '      --        , max(etv.etvcodigo) '
      ''
      ''
      '  FROM etd'
      '  inner JOIN tpe ON etd.tpecodigo = tpe.tpecodigo'
      '  inner JOIN etv ON etd.etdcodigo = etv.etdcodigo'
      '  inner JOIN tvi ON etv.tvicodigo = tvi.tvicodigo'
      '  inner JOIN tse ON tse.tsecodigo = etd.tsecodigo'
      ''
      
        '  inner JOIN edr ON etd.etdcodigo = edr.etdcodigo AND (edr.tedco' +
        'digo=1 or edr.tedcodigo=2)'
      
        '  inner JOIN etf ON etd.etdcodigo = etf.etdcodigo and (etf.ttfco' +
        'digo=1 or etf.ttfcodigo=8)'
      ''
      '--   left join grt ON etd.grtcodigo = grt.grtcodigo'
      ''
      '  WHERE etd.tsecodigo<>9 '
      '   '
      ''
      '-- filtros'
      '')
    ReadOnly = True
    BeforeOpen = uqtabelaBeforeOpen
    Left = 372
    Top = 208
