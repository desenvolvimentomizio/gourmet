  object qrfiSaldo: TUniQuery
    SQL.Strings = (
      'SET @tmfPagamentoCapital = 11;'
      'SET @tmfRecebimentoCapital = 21;'
      ''
      'SET @srfQuitado = 2;'
      ''
      'SELECT'
      '  /*Saldo Capital*/'
      '  SUM(rfi.rfivalor - tmfi.PagamentosCapital) AS rfisaldocapital'
      'FROM rfi'
      '  LEFT JOIN (SELECT'
      '      mfi.rfichave,'
      '      /*Capital*/'
      
        '      SUM(IF(mfi.tmfcodigo IN (@tmfPagamentoCapital, @tmfRecebim' +
        'entoCapital), mfivalor, 0)) AS PagamentosCapital'
      '    FROM mfi'
      '    GROUP BY mfi.rfichave) tmfi'
      '    ON tmfi.rfichave = rfi.rfichave'
      'WHERE rfi.tfdcodigo = 2'
      'AND rfi.etdcodigo = :etdcodigo')
    Left = 814
    Top = 231
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'etdcodigo'
        Value = nil
      end>
    object qrfiSaldorfisaldocapital: TFloatField
      FieldName = 'rfisaldocapital'
    end
  end
