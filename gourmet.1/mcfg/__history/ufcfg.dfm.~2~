  inherited registro: TUniQuery
    SQL.Strings = (
      'SELECT cfg.cfgcodigo'
      '     , cfg.flacodigo'
      '     , cfg.cfgidentificacao'
      '     , cfg.cfgdtinictb'
      '     , cfg.cfgdtfinctb'
      '     , cfg.cfgusasped'
      '     , cfg.cfgusavnd'
      '     , cfg.cfgusacpr'
      '     , cfg.cfgusactb'
      '     , cfg.cfgusacpa'
      '     , cfg.cfgusacre'
      '     , cfg.cfgusabol'
      '     , cfg.cfgusasrv'
      '     , cfg.cfgusanfe'
      '     , cfg.cfgusapdv'
      '     , cfg.cfgusanfc'
      '     , cfg.cfgusarh'
      '     , cfg.cfgctacaixa'
      '     , cfg.cfgusaadc'
      '     , cfg.cfgusache'
      '     , cfg.cfgusatelentre'
      '     , cfg.cfgusagou'
      '     , cfg.cfgatuctb'
      '     , cfg.cfgusasrv'
      '     , cfg.cfgeminclusao'
      '     , cfg.cfgtodasentidades'
      '     , cfg.cfgctacodigopix'
      '     , cfg.cfgetdcodigopix'
      ''
      '  FROM cfg'
      ' WHERE cfg.flacodigo = :flacodigo')
    Left = 640
    Top = 332
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'flacodigo'
        Value = nil
