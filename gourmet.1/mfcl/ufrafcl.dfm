  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  fclcodigo,'
      '  fclidentificacao,'
      '  fcldatainicial,'
      '  fcldatafinal,'
      '  fcl.evfcodigo,'
      '  evf.evfidentificacao,'
      '  fcl.phgcodigo,'
      '  phg.phgidentificacao,'
      '  tth.tthidentificacao,'
      '  fcl.tthcodigo,'
      '  fclcor,'
      '  tloidentificacao,'
      
        '  if (tth.tthcodigo=14, (select afcvalor from afc where afc.fclc' +
        'odigo=fcl.fclcodigo and afcdatafinal='#39'9999-12-31'#39' order by afcch' +
        'ave desc limit 1),fclvalorbase) fclvalorbase, '
      '  senidentificacao pagadsr'
      'FROM fcl'
      '  INNER JOIN evf ON fcl.evfcodigo=evf.evfcodigo'
      '  INNER JOIN phg ON fcl.phgcodigo=phg.phgcodigo'
      '  INNER JOIN tth on fcl.tthcodigo=tth.tthcodigo'
      '  INNER JOIN tlo on fcl.tlocodigo=tlo.tlocodigo'
      '  INNER JOIN sen on fcl.fclpagadsr=sen.sencodigo'
      '')
    object uqtabelafclcodigo: TIntegerField
      DisplayLabel = 'C'#243'digo'
