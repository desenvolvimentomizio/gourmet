  inherited registro: TUniQuery
    SQL.Strings = (
      'SELECT pro.procodigo'
      '     , pro.pronome'
      '     , pro.pronomereduzido'
      '     , pro.marcodigo'
      '     , pro.grpcodigo'
      '     , pro.trbcodigo'
      '     , pro.icmcodigo'
      '     , pro.sipcodigo'
      '     , pro.tpocodigo'
      '     , pro.prominimo'
      '     , pro.proproprio'
      '     , pro.pronumserie'
      '     , pro.proncm'
      '     , pro.unicodigo'
      '     , pro.procomposto'
      '     , pro.probalanca'
      '     , pro.provalidade'
      '     , pro.protecla'
      '     , pro.proestoque'
