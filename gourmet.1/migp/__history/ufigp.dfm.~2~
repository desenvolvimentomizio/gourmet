  object grp: TUniQuery
    SQL.Strings = (
      
        'select grpcodigo,concat(tgridentificacao, '#39' - '#39', grpidentificaca' +
        'o) AS grpidentificacao from grp,tgr where grp.tgrcodigo=tgr.tgrc' +
        'odigo')
    Left = 672
    Top = 136
    object grpgrpcodigo: TIntegerField
      AutoGenerateValue = arAutoInc
      FieldName = 'grpcodigo'
    end
    object grpgrpidentificacao: TStringField
      DisplayWidth = 45
      FieldName = 'grpidentificacao'
      Size = 45
    end
  end
