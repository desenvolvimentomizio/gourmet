      '  puncodigo,'
      '  itmcontendo,'
      '  cfocfopdestinacao,'
      '  unicodigobase,'
      '  itmcusto,'
      '  itmfrete,'
      '  itmseguro,'
      '  itmoutras,'
      '  itmquanticontada'
      'FROM itm'
      'WHERE itmchave = :itmchave AND'
      'meschave = :meschave')
    BeforePost = registroBeforePost
    Left = 556
    Top = 40
    ParamData = <
