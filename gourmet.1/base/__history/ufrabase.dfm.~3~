  object rel: TUniQuery
    SmartFetch.Enabled = True
    SQL.Strings = (
      'SELECT DISTINCT mre.bplcodigo'
      '              , rel.reltitulo'
      '              , rel.relnomearquivo'
      '              , rel.relarquivo'
      '              , rel.relidentificacao'
      '              , bpl.bplfranome'
      '              , rel.relcodigo'
      'FROM mre'
      '  INNER JOIN rel'
      '    ON mre.relcodigo = rel.relcodigo'
      '  INNER JOIN bpl'
      '    ON mre.bplcodigo = bpl.bplcodigo'
      '  LEFT JOIN rau'
      '    ON rel.relcodigo = rau.relcodigo'
      '    AND rau.rauativo = 1'
      'WHERE bpl.bplfranome = :franome'
      'AND rel.relsituacao = 1'
      'AND IF(:clbcodigo = 1, True, rau.clbcodigo = :clbcodigo)'
      'ORDER BY rel.relidentificacao')
    Left = 480
    Top = 312
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'franome'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'clbcodigo'
        Value = nil
      end>
