  object its: TUniQuery
    SQL.Strings = (
      'SELECT DISTINCT mis.bplcodigo'
      '              , its.itsidentificacao'
      '              , bpl.bplfranome'
      '              , its.itscodigo'
      '              , its.itspdf'
      'FROM mis'
      '  INNER JOIN its'
      '    ON mis.itscodigo = its.itscodigo'
      '  INNER JOIN bpl'
      '    ON mis.bplcodigo = bpl.bplcodigo'
      'WHERE bpl.bplfranome = :franome'
      'ORDER BY its.itsidentificacao')
    Left = 368
    Top = 477
