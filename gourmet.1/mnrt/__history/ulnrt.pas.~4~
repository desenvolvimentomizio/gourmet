unit ulnrt;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, ulfrmbase, Vcl.StdCtrls, Vcl.Buttons,
  Vcl.ExtCtrls, ufrabase, ufranrt,uPegaBase, Data.DB, Xml.xmldom, Xml.XMLIntf,
  Xml.XMLDoc, System.ImageList, Vcl.ImgList, VirtualTable, MemDS, DBAccess, Uni,
  Vcl.Menus, System.Actions, Vcl.ActnList, Vcl.Grids, Vcl.DBGrids,
  Vcl.Imaging.jpeg, Vcl.Mask, Vcl.DBCtrls, Vcl.Imaging.pngimage;

type
  Tlnrt = class(Tfranrt)
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  lnrt: Tlnrt;


implementation




{$R *.dfm}


function formu(pCargaFrame: TCargaFrame): string;
begin
  pCargaFrame.Titulo := vPlTitMdl;
  lnrt := Tlnrt.Create(pCargaFrame);
  try
    lnrt.ShowModal;
    Result := lnrt.franrt.vretorno;
  finally
    lnrt.Free;
  end;
end;

exports formu;

end.
