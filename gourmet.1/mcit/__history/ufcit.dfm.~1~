  inherited registro: TUniQuery
    SQL.Strings = (
      'select citcodigo,'
      'tcicodigo,'
      'trmcodigo from cit'
      'where  citcodigo=:citcodigo and cit.trmcodigo=:trmcodigo')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'citcodigo'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'trmcodigo'
        Value = nil
      end>
