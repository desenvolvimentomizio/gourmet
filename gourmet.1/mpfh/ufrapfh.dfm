  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  pfh.pfhchave,'
      '  pfh.flhchave,'
      '  pfh.tlocodigo,'
      '  pfh.vhfcodigo,'
      '  pfh.pfhhoras,'
      '  tlo.tloidentificacao,'
      '  fcl.fclidentificacao,'
      '  vhf.vhfidentificacao,'
      '  etd.etdidentificacao,'
      '  dtr.dtrdataadmissao,'
      '  vhf.etdcodigo,'
      '  flh.flhidentificacao,'
      '  vhf.anfcodigo,'
      '  anf.anfidentificacao,'
      '  ffh.ffhidentificacao '
      '   '
      'FROM pfh,'
      '     tlo,'
      '     fcl,'
      '     vhf,'
      '     etd,'
      '     dtr,'
      '     flh,'
      '     anf,'
      '     ffh'
      'WHERE pfh.tlocodigo = tlo.tlocodigo'
      'AND pfh.vhfcodigo = vhf.vhfcodigo'
      'AND vhf.fclcodigo = fcl.fclcodigo'
      'AND vhf.etdcodigo = etd.etdcodigo'
      'AND etd.etdcodigo = dtr.etdcodigo'
      'AND dtr.ffhcodigo = ffh.ffhcodigo'
      'AND pfh.flhchave = flh.flhchave'
      'AND vhf.anfcodigo = anf.anfcodigo')
