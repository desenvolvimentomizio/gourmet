  object cge: TUniQuery
    SQL.Strings = (
      '/* s'#243' do contas a pagar */'
      ''
      ''
      'SELECT'
      '  ccgcodigo,'
      '  ccgestrutural  ccgidentificacao,'
      '  seacodigo,'
      ''
      
        '  IFNULL( (SELECT SUM(mcgvalor)  FROM mcg, cco, cta WHERE mcgsit' +
        'uacao=0 and mcg.mcgchaveorigem=cco.ccochave and cta.ctacodigo=cc' +
        'o.ctacodigo and cta.ctafluxo=1  and mcgtabela='#39'cco'#39' and  mcg.mcg' +
        'competencia = CONCAT('#39'01'#39','#39'-'#39',(SELECT SUBSTRING(cgeano,3,2) FROM' +
        ' cge WHERE cgechave=:cgechave ) ) and mcg.ccgcodigo=ccg.ccgcodig' +
        'o GROUP BY ccgcodigo),0) AS cgerealizado01,'
      
        '  IFNULL( (select dcgprevisao FROM dcg WHERE dcg.ccgcodigo=ccg.c' +
        'cgcodigo and dcg.dcgano=:cgeano and dcgmes=1 LIMIT 1),0) cgeprev' +
        'isto01,'
      
        '  IFNULL( (select dcgprevisao FROM dcg WHERE dcg.ccgcodigo=ccg.c' +
        'cgcodigo and dcg.dcgano=:cgeano and dcgmes=1 LIMIT 1),0)-IFNULL(' +
        ' (SELECT SUM(mcgvalor)  FROM mcg WHERE mcgtabela='#39'cco'#39' and  mcg.' +
        'mcgcompetencia = CONCAT('#39'01'#39','#39'-'#39',(SELECT SUBSTRING(cgeano,3,2) F' +
        'ROM cge WHERE cgechave=:cgechave))  and mcg.ccgcodigo=ccg.ccgcod' +
        'igo GROUP BY ccgcodigo),0)  AS ogesaldo01, '
      ''
      
        '  IFNULL( (SELECT SUM(mcgvalor)  FROM mcg, cco, cta WHERE mcgsit' +
        'uacao=0 and  mcg.mcgchaveorigem=cco.ccochave and cta.ctacodigo=c' +
        'co.ctacodigo and cta.ctafluxo=1  and  mcgtabela='#39'cco'#39' and  mcg.m' +
        'cgcompetencia = CONCAT('#39'02'#39','#39'-'#39',(SELECT SUBSTRING(cgeano,3,2) FR' +
        'OM cge WHERE cgechave=:cgechave) )  and mcg.ccgcodigo=ccg.ccgcod' +
        'igo GROUP BY ccgcodigo),0) AS cgerealizado02,'
      
        '  IFNULL( (select dcgprevisao FROM dcg WHERE dcg.ccgcodigo=ccg.c' +
        'cgcodigo and dcg.dcgano=:cgeano and dcgmes=2 LIMIT 1),0) cgeprev' +
        'isto02,'
      
        '  IFNULL( (select dcgprevisao FROM dcg WHERE dcg.ccgcodigo=ccg.c' +
        'cgcodigo and dcg.dcgano=:cgeano and dcgmes=2 LIMIT 1),0)-IFNULL(' +
        ' (SELECT SUM(mcgvalor)  FROM mcg WHERE mcgtabela='#39'cco'#39' and  mcg.' +
        'mcgcompetencia = CONCAT('#39'02'#39','#39'-'#39',(SELECT SUBSTRING(cgeano,3,2) F' +
        'ROM cge WHERE cgechave=:cgechave))  and mcg.ccgcodigo=ccg.ccgcod' +
        'igo GROUP BY ccgcodigo),0)  AS ogesaldo02, '
      ''
      
        '  IFNULL( (SELECT SUM(mcgvalor)  FROM mcg, cco, cta WHERE mcgsit' +
        'uacao=0 and  mcg.mcgchaveorigem=cco.ccochave and cta.ctacodigo=c' +
        'co.ctacodigo and cta.ctafluxo=1  and  mcgtabela='#39'cco'#39' and  mcg.m' +
