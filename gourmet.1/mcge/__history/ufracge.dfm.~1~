      'SELECT'
      '  ccgcodigo,'
      '  ccgestrutural  ccgidentificacao,'
      '  seacodigo,'
      ''
      
        '  IFNULL( (SELECT SUM(mcgvalor)  FROM mcg, cco, cta WHERE mcgsit' +
        'uacao=0 and mcg.mcgchaveorigem=cco.ccochave and cta.ctacodigo=cc' +
        'o.ctacodigo and cta.ctafluxo=1  and mcgtabela='#39'cco'#39' and  mcg.mcg' +
        'data BETWEEN CONCAT((SELECT cgeano FROM cge WHERE cgechave=:cgec' +
        'have ) ,'#39'-01-01'#39') AND CONCAT((SELECT cgeano FROM cge WHERE cgech' +
        'ave=:cgechave) ,'#39'-01-31'#39') and mcg.ccgcodigo=ccg.ccgcodigo GROUP ' +
        'BY ccgcodigo),0) AS cgerealizado01,'
      
        '  IFNULL( (select dcgprevisao FROM dcg WHERE dcg.ccgcodigo=ccg.c' +
        'cgcodigo and dcg.dcgano=:cgeano and dcgmes=1 LIMIT 1),0) cgeprev' +
        'isto01,'
      
        '  IFNULL( (select dcgprevisao FROM dcg WHERE dcg.ccgcodigo=ccg.c' +
        'cgcodigo and dcg.dcgano=:cgeano and dcgmes=1 LIMIT 1),0)-IFNULL(' +
        ' (SELECT SUM(mcgvalor)  FROM mcg WHERE mcgtabela='#39'cco'#39' and  mcg.' +
        'mcgdata BETWEEN CONCAT((SELECT cgeano FROM cge WHERE cgechave=:c' +
        'gechave) ,'#39'-01-01'#39') AND CONCAT((SELECT cgeano FROM cge WHERE cge' +
        'chave=:cgechave) ,'#39'-01-31'#39') and mcg.ccgcodigo=ccg.ccgcodigo GROU' +
        'P BY ccgcodigo),0)  AS ogesaldo01, '
      ''
      
        '  IFNULL( (SELECT SUM(mcgvalor)  FROM mcg, cco, cta WHERE mcgsit' +
        'uacao=0 and  mcg.mcgchaveorigem=cco.ccochave and cta.ctacodigo=c' +
        'co.ctacodigo and cta.ctafluxo=1  and  mcgtabela='#39'cco'#39' and  mcg.m' +
        'cgdata BETWEEN CONCAT((SELECT cgeano FROM cge WHERE cgechave=:cg' +
        'echave) ,'#39'-02-01'#39') AND CONCAT((SELECT cgeano FROM cge WHERE cgec' +
