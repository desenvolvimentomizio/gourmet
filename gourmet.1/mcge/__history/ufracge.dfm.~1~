  object rfi: TUniQuery
    SQL.Strings = (
      'SELECT sum(dtlvalor) as modalidade_valor FROM '
      ' rfi'
      '  INNER JOIN mfi ON rfi.rfichave=mfi.rfichave '
      '  INNER JOIN mlt ON mfi.mfichave=mlt.mfichave'
      '  INNER JOIN dtl ON mlt.ltechave=dtl.ltechave'
      '  WHERE tficodigo=6'
      '  AND tfdcodigo=2'
      '  AND tmfcodigo=21'
      '  and dtl.mdacodigo=:mdacodigo'
      '  and dtl.dtlregistro between :inicial and :final')
    Left = 934
    Top = 525
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'mdacodigo'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'inicial'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'final'
        Value = nil
      end>
