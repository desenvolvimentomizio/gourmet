  object cre: TUniQuery
    SQL.Strings = (
      'SET lc_time_names = '#39'pt_BR'#39';'
      'SET @datainicial =:dtinicial;'
      'SET @datafinal = :dtfinal;'
      ''
      'SELECT distinct '
      '      SUM(rfi.rfibaixadocapital) AS total '
      'FROM v_rfi rfi '
      '     INNER join etd on rfi.etdcodigo=etd.etdcodigo '
      '     left join rfm on rfi.rfichave=rfm.rfichave '
      '     left join mor on rfm.meschave=mor.meschave '
      '     left join mes on rfm.meschave=mes.meschave  '
      ' '
      'WHERE rfi.tficodigo<>6 and (rfi.tfdcodigo = 2'
      
        '  and  (rfi.srfcodigo=1 OR rfi.srfcodigo=2 OR rfi.srfcodigo=3)) ' +
        'AND rfi.rfidtultbaixa BETWEEN (@datainicial) and (@datafinal) '
      ''
      ''
      '')
    Left = 854
    Top = 517
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dtinicial'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'dtfinal'
        Value = nil
      end>
    object cretotal: TCurrencyField
      FieldName = 'total'
    end
  end
