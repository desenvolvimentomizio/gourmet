  object dtl: TUniQuery
    SQL.Strings = (
      'SELECT '
      ''
      '  sum( itmtotal) dtlvalor'
      '  '
      ''
      'FROM rfm'
      '  INNER JOIN mes'
      '    ON rfm.meschave = mes.meschave'
      '    AND rfm.flacodigo = mes.flacodigo'
      ''
      '  inner JOIN itm ON itm.meschave=mes.meschave '
      '  INNER JOIN pro ON itm.procodigo=pro.procodigo '
      '  INNER JOIN dpr ON pro.dprcodigo=dpr.dprcodigo'
      ''
      '  INNER JOIN rfi'
      '    ON rfm.rfichave = rfi.rfichave'
      '    AND rfi.flacodigo = mes.flacodigo'
      '  INNER JOIN mfi'
      '    ON mfi.rfichave = rfi.rfichave'
      '    AND mfi.flacodigo = mes.flacodigo'
      '  LEFT JOIN mlt'
      '    ON mlt.mfichave = mfi.mfichave'
      '    AND mes.flacodigo = mlt.flacodigo'
      '  INNER JOIN dtl'
      '    ON dtl.ltechave = mlt.ltechave'
      '    AND mlt.flacodigo = dtl.flacodigo'
      '  LEFT JOIN mda'
      '    ON dtl.mdacodigo = mda.mdacodigo'
      '  LEFT JOIN lte'
      '    ON mlt.ltechave = lte.ltechave'
      '    AND mes.flacodigo = lte.flacodigo'
      'WHERE rfi.tfdcodigo IN (2, 32)'
      'AND IF(rfi.tfdcodigo = 2, mfi.tmfcodigo = 2, mfi.tmfcodigo)'
      'AND mes.tdfcodigo <> '#39'RF'#39
      'AND mes.mesrefeicao = 0'
      'AND mda.mdacodigo in (4,7,15,16,17,18)'
      'AND (mesemissao>=:primeirodia and mesemissao<=:ultimodia)'
      'AND dpr.ccgcodigo=:ccgcodigo'
      'AND mes.flacodigo = :flacodigo'
      
        'AND IF((dtl.mdacodigo = 4 OR dtl.mdacodigo = 5),if((select adcli' +
        'quidaautomatico from adc limit 1)=0 ,tmfcodigo = 21,tmfcodigo = ' +
        '2), tmfcodigo = 21)')
    Left = 598
    Top = 373
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'primeirodia'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'ultimodia'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'ccgcodigo'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'flacodigo'
        Value = nil
      end>
    object dtldtlvalor: TFloatField
      FieldName = 'dtlvalor'
    end
  end
  object mda: TUniQuery
    SQL.Strings = (
      'set @datainicial=:inicial;'
      'set @datafinal=:final;  '
      'SELECT'
      #9'vendas.mdacodigo AS modalidade_id,'
      #9'vendas.mdaidentificacao AS modalidade_nome,'
      #9'ROUND(SUM(vendas.dtlvalor), 2) AS modalidade_valor'
      'FROM'
      #9'(SELECT'
      #9#9'dtl.dtlchave,'
      #9#9'mda.mdacodigo,'
      #9#9'mda.mdaidentificacao,'
      #9#9'dtl.dtlvalor'
      #9'FROM'
      #9#9'mes'
      #9#9'INNER JOIN rfm ON mes.meschave = rfm.meschave'
      #9#9'INNER JOIN rfi ON rfm.rfichave = rfi.rfichave'
      #9#9'INNER JOIN mfi ON rfi.rfichave = mfi.rfichave'
      #9#9'INNER JOIN mlt ON mfi.mfichave = mlt.mfichave'
      #9#9'INNER JOIN lte ON mlt.ltechave = lte.ltechave'
      #9#9'INNER JOIN dtl ON lte.ltechave = dtl.ltechave'
      #9#9'INNER JOIN mda ON dtl.mdacodigo = mda.mdacodigo'
      #9#9'INNER JOIN itm ON itm.meschave = mes.meschave'
      #9#9'INNER JOIN toe ON toe.toecodigo = itm.toecodigo'
      '  INNER JOIN (SELECT'
      
        #9#9#9#9#9#9'IFNULL(MIN(CAST(CONCAT(ccx.ccxdataber, '#39' '#39', ccx.ccxhoraabe' +
        'r) AS DATETIME)),'
      
        #9#9#9#9#9#9#9'CONCAT(@datainicial, '#39' '#39', (SELECT cfgmgouhoravirada FROM ' +
        'cfgmgou))) AS ccxdatahoraabert'
      #9#9#9#9#9'FROM'
      #9#9#9#9#9#9'ccx'
      #9#9#9#9#9#9'INNER JOIN cta ON ccx.ctacodigo = cta.ctacodigo'
      #9#9#9#9#9'WHERE'
      #9#9#9#9#9#9'ccxdataber >= @datainicial'
      #9#9#9#9#9#9'AND ccxdataber <= @datafinal'
      
        #9#9#9#9#9'ORDER BY ccx.ccxchave DESC) AS dtinicial ON mes.mesinclusao' +
        ' >= dtinicial.ccxdatahoraabert'
      #9#9'INNER JOIN (SELECT'
      #9#9#9#9#9#9'IFNULL((SELECT'
      #9#9#9#9#9#9#9#9#9'CONCAT(ccxdatafecha, '#39' '#39', ccxhorafecha)'
      #9#9#9#9#9#9#9#9'FROM'
      #9#9#9#9#9#9#9#9#9'ccx WHERE ccxchave = (SELECT IFNULL(MAX(ccxchave), 0)'
      #9#9#9#9#9#9#9#9#9#9#9#9#9#9#9'FROM ccx WHERE ccxdataber <= @datafinal)), '
      
        #9#9#9#9#9#9'CONCAT(DATE_ADD(@datafinal, INTERVAL 1 DAY), '#39' '#39', (SELECT ' +
        'cfgmgouhoravirada FROM cfgmgou))) AS ccxdatahorafecha) AS dtfina' +
        'l ON mes.mesinclusao <= dtfinal.ccxdatahorafecha           '
      #9'WHERE'
      #9#9'toe.ttocodigo = 2'
      #9#9'AND mes.sdecodigo = '#39'00'#39
      #9#9'AND lte.tfdcodigo = 32'
      #9#9'AND lte.ltesituacao = 0'
      #9#9'AND ISNULL((SELECT mlt.ltechave'
      #9#9#9#9'   FROM mes'
      #9#9#9#9'  INNER JOIN rfm ON mes.meschave = rfm.meschave'
      #9#9#9#9'  INNER JOIN mfi ON rfm.rfichave = mfi.rfichave'
      #9#9#9#9'  INNER JOIN mlt ON mfi.mfichave = mlt.mfichave'
      #9#9#9#9'  WHERE mes.mesrefeicao = 1                        '
      #9#9#9#9#9'AND mlt.ltechave = lte.ltechave'
      #9#9#9'   GROUP BY mlt.ltechave))'
      #9#9'AND ISNULL((SELECT mlt.ltechave'
      #9#9#9#9'   FROM mes'
      #9#9#9#9'  INNER JOIN rfm ON mes.meschave = rfm.meschave'
      #9#9#9#9'  INNER JOIN mfi ON rfm.rfichave = mfi.rfichave'
      #9#9#9#9'  INNER JOIN mlt ON mfi.mfichave = mlt.mfichave'
      #9#9#9#9'  WHERE mes.sdecodigo NOT IN ('#39'00'#39', '#39'01'#39')'
      #9#9#9#9#9'AND mlt.ltechave = lte.ltechave'
      #9#9#9'   GROUP BY mlt.ltechave))'
      #9#9'AND ISNULL(IF(lte.tfdcodigo = 3, (SELECT cco.toccodigo '
      #9#9#9#9#9'  FROM cco '
      #9#9#9#9#9' INNER JOIN clt ON cco.ccochave = clt.ccochave '
      #9#9#9#9#9' WHERE clt.ltechave = lte.ltechave'
      #9#9#9#9#9'   AND cco.toccodigo NOT IN (7, 8, 9, 10)'
      #9#9#9#9#9' GROUP BY clt.ltechave), NULL))'
      #9'GROUP BY dtl.dtlchave) AS vendas'
      'GROUP BY vendas.mdacodigo;')
    Left = 934
    Top = 477
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'inicial'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'final'
        Value = nil
      end>
    object mdamodalidade_id: TIntegerField
      FieldName = 'modalidade_id'
      Required = True
    end
    object mdamodalidade_nome: TStringField
      FieldName = 'modalidade_nome'
      Required = True
      Size = 30
    end
    object mdamodalidade_valor: TFloatField
      FieldName = 'modalidade_valor'
      ReadOnly = True
    end
  end
