        'e) AS ccxdatahoraabert,'
      
        '    CAST(CONCAT(ccx.ccxdatafecha, '#39' '#39', ccx.ccxhorafecha) AS date' +
        'time) AS ccxdatahorafecha'
      '  FROM ccx'
