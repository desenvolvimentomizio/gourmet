      '  acgprevisto08,'
      '  acgrealizado08,'
      '  acgsaldo08,'
      '  acgpercentual08,'
      '  acgprevisto09,'
      '  acgrealizado09,'
      '  acgsaldo09,'
      '  acgpercentual09,'
      '  acgprevisto10,'
      '  acgrealizado10,'
      '  acgsaldo10,'
      '  acgpercentual10,'
      '  acgprevisto11,'
      '  acgrealizado11,'
      '  acgsaldo11,'
      '  acgpercentual11,'
      '  acgprevisto12,'
      '  acgrealizado12,'
      '  acgsaldo12,'
      '  acgpercentual12,'
      '  acgprevistogeral,'
      '  acgrealizadogeral,'
      '  acgsaldogeral,'
      '  acgpercentualgeral,'
      '  seacodigo,'
      
        ' (select cedcodigo from ccg where ccg.ccgcodigo=acg.ccgcodigo)ce' +
        'dcodigo '
      'FROM acg'
      'where cgechave=:cgechave '
      
        'order by concat((select ccgestrutural from ccg where ccg.ccgcodi' +
        'go=acg.ccgcodigo), '#39' '#39', (select ccgidentificacao from ccg where ' +
        'ccg.ccgcodigo=acg.ccgcodigo))')
