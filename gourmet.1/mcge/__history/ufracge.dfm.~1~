  object dcg: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  dcg.dcgchave,'
      '  dcg.cgechave,'
      '  dcg.ccgcodigo,'
      '  dcg.dcgmes,'
      '  ccg.ccgidentificacao,'
      '  dcg.dcgprevisao,'
      '  dcg.dcgrealizado,'
      '  (dcg.dcgprevisao-dcg.dcgrealizado) dcgsaldo,'
      
        '  (((dcg.dcgrealizado-dcg.dcgrealizado)/dcg.dcgprevisao)*100) dc' +
        'gpercentual,'
      '  dcg.dcgpercotimista,'
      '  dcg.dcgperpessimista,'
      '  dcg.dcgsituacao,'
      '  dcg.clbcodigo,'
      '  dcg.dcgregistro'
      'FROM dcg, ccg where cgechave=:cgechave'
      '  AND  dcg.ccgcodigo=ccg.ccgcodigo'
      'order by dcg.ccgcodigo, dcgmes')
    Left = 899
    Top = 317
