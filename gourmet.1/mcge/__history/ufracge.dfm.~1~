  object acg: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  acgchave,'
      '  cgechave,'
      '  ccgcodigo,'
      '  ccgidentificacao ccgestrutural,'
      
        '  concat((select ccgestrutural from ccg where ccg.ccgcodigo=acg.' +
        'ccgcodigo), '#39' '#39', (select ccgidentificacao from ccg where ccg.ccg' +
        'codigo=acg.ccgcodigo)) ccgtitulo,'
      '  ccgidentificacao,'
      '  acgprevisto01,'
      '  acgrealizado01,'
      '  acgsaldo01,'
      '  acgpercentual01,'
      '  acgprevisto02,'
