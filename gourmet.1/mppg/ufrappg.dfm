      'SELECT'
      '  tfi.tfiidentificacao,'
      '  ipg.tficodigo,'
      '  ipg.rfichave,'
      '  ipg.ipgchave,'
      '  ipg.ppgchave,'
      '  ipg.rfivalor,'
      '  ipg.ipgnumerocheque,'
      '  clb.clbidentificacao,'
      '  ipg.clbcodigo,'
      '  v_rfi.etdcodigo,'
      '  v_rfi.rfiemissao,'
