unit ufraehf;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, ufrabase, Data.DB, System.ImageList,
  Vcl.ImgList, Vcl.ExtCtrls, VirtualTable, MemDS, DBAccess, Uni, Vcl.Menus,
  System.Actions, Vcl.ActnList, Vcl.StdCtrls, Vcl.Buttons, Vcl.Grids,
  Vcl.DBGrids, Vcl.Imaging.jpeg, Vcl.Mask, Vcl.DBCtrls, Vcl.Imaging.pngimage,uPegaBase,
  DBGrid2Excel;

type
  Tfraehf = class(Tfrabase)
    uqtabelaehfchave: TIntegerField;
    uqtabelaflhchave: TIntegerField;
    uqtabelavhfcodigo: TIntegerField;
    uqtabelatthcodigo: TIntegerField;
    uqtabelaflhidentificacao: TStringField;
    uqtabelavhfidentificacao: TStringField;
    uqtabelatthidentificacao: TStringField;
    uqtabelaehfhoras: TStringField;
    uqtabelaetdidentificacao: TStringField;
    ActImportarHoras: TAction;
    procedure ActAlterarExecute(Sender: TObject);
    procedure ActIncluirExecute(Sender: TObject);
    procedure ActImportarHorasExecute(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  fraehf: Tfraehf;



    // Início ID do Módulo fraehf
const
  vPlIdMd = '00.00.00.000-01';
  vPlTitMdl = 'Registro de Horas';

  // Fim ID do Módulo fraehf


implementation

uses
  ufehf, ufimpehf;

{$R *.dfm}


function formuFrame(pCargaFrame: TCargaFrame): string;
begin
  pCargaFrame.IdModulo := vPlIdMd;
  pCargaFrame.Titulo := vPlTitMdl;
  fraehf := Tfraehf.Create(pCargaFrame);
end;

procedure defineacesso(pCargaFrame: TCargaFrame);
begin
  pCargaFrame.Titulo := vPlTitMdl;
  fraehf := Tfraehf.Create(pCargaFrame);
  try
    fraehf.CriaAcoesDeAcesso;
  finally
    fraehf.Free;
  end;
end;

exports formuFrame, defineacesso;


procedure Tfraehf.ActAlterarExecute(Sender: TObject);
begin
  inherited;
 CriaFormulario(Tfehf, uqtabelaehfchave.AsString , '');
end;

procedure Tfraehf.ActImportarHorasExecute(Sender: TObject);
begin
  inherited;
   CriaFormulario(Tfimpehf, '' , '');
end;

procedure Tfraehf.ActIncluirExecute(Sender: TObject);
begin
  inherited;
 CriaFormulario(Tfehf, '' , '');
end;

end.
