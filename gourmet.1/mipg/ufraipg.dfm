  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  ipg.ipgchave,'
      '  tfi.tfiidentificacao,'
      '  ipg.tficodigo,'
      '  ipg.rfichave,'
      '  ipg.ppgchave,'
      '  ipg.rfivalor,'
      '  ipg.ipgnumerocheque,'
      '  clb.clbidentificacao,'
      '  ipg.clbcodigo,'
      '  v_rfi.etdcodigo,'
      '  v_rfi.rfiemissao,'
      '  v_rfi.rfivencimento,'
      '  v_rfi.rfinumero,'
      '  v_rfi.rfihistorico,'
      '  v_rfi.etdidentificacao,'
      '  v_rfi.rfidias,'
      '  v_rfi.rfijuros,'
      '  v_rfi.rfimoradia,'
      '  v_rfi.rfisaldogeral'
      'FROM v_rfi'
      '  INNER JOIN ipg'
      '    ON v_rfi.rfichave = ipg.rfichave'
      '  INNER JOIN tfi'
      '    ON tfi.tficodigo = ipg.tficodigo'
      '  INNER JOIN clb'
      '    ON clb.clbcodigo = ipg.clbcodigo'
      'where ipg.ppgchave=:ppgchave')
