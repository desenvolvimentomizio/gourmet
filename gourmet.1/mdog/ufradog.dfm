  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  dog.dogchave,'
      '  dog.ogechave,'
      '  dog.pcgcodigo,'
      '  pcg.pcgidentificacao,'
      '  pcg.pcgestrutural,'
      '  dog.dogprevisao,'
      '  dog.dogrealizado,'
      '  (dog.dogprevisao-dog.dogrealizado) dogsaldo,'
      
        '  (((dog.dogrealizado-dog.dogrealizado)/dog.dogprevisao)*100) do' +
        'gpercentual,'
      '  dog.dogpercotimista,'
      '  dog.dogperpessimista,'
      '  dog.dogsituacao,'
      '  dog.dogmes,'
      '  dog.dogano,'
      '  dog.clbcodigo,'
      '  dog.dogregistro'
      'FROM dog, pcg where ogechave=:ogechave'
      '  AND  dog.pcgcodigo=pcg.pcgcodigo')
    MasterSource = Dmeses
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ogechave'
        Value = nil
      end>
