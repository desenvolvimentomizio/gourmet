unit GourmetAIQ.Model.Entity.PedidoAIQGourmet;

interface

{$TYPEINFO ON}

Uses
  SimpleAttributes;

type

  TPedidoAIQ = Class
  private
    faiqpedido: string;
    procedure setaiqpedido(const Value: string);
  published
    property aiqpedido: string read faiqpedido write setaiqpedido;
  end;

type

  [Tabela('aiq')]
  TPedidoAIQGourmet = Class
  private
    faiqchave: integer;
    faiqpedido: string;
    faiqstatus: string;
    faiqjson: string;
    forcchave: integer;
    fetdidentificacao: string;
    fimmnumepedido: string;
    fimmhoraimpresso: string;
    fcznchave: integer;

    procedure setaiqchave(const Value: integer);
    procedure setaiqpedido(const Value: string);
    procedure setaiqstatus(const Value: string);
    procedure setaiqjson(const Value: string);

    procedure setorcchave(const Value: integer);
    procedure setetdidentificacao(const Value: string);
    procedure setimmnumepedido(const Value: string);
    procedure setimmhoraimpresso(const Value: string);
    procedure setcznchave(const Value: integer);


  public
    constructor create;
    destructor destroy; override;
  published
    [campo('aiqchave'), PK]
    property aiqchave: integer read faiqchave write setaiqchave;

    [campo('aiqpedido')]
    property aiqpedido: string read faiqpedido write setaiqpedido;

    [campo('aiqstatus')]
    property aiqstatus: string read faiqstatus write setaiqstatus;

    [campo('aiqjson')]
    property aiqjson: string read faiqjson write setaiqjson;

    property orcchave: integer read forcchave write setorcchave;
    property etdidentificacao: string read fetdidentificacao write setetdidentificacao;
    property immnumepedido: string read fimmnumepedido write setimmnumepedido;

    property immhoraimpresso: string read fimmhoraimpresso write setimmhoraimpresso;
    property cznchave: integer read fcznchave write setcznchave;


  end;

implementation

{ TPedidoAIQGourmet }

constructor TPedidoAIQGourmet.create;
begin

end;

destructor TPedidoAIQGourmet.destroy;
begin

  inherited;
end;

procedure TPedidoAIQGourmet.setaiqchave(const Value: integer);
begin
  faiqchave := Value;
end;

procedure TPedidoAIQGourmet.setaiqjson(const Value: string);
begin

  faiqjson := Value;
end;

procedure TPedidoAIQGourmet.setaiqpedido(const Value: string);
begin
  faiqpedido := Value;

end;

procedure TPedidoAIQGourmet.setaiqstatus(const Value: string);
begin
  faiqstatus := Value;
end;

procedure TPedidoAIQGourmet.setcznchave(const Value: integer);
begin
  fcznchave := Value;
end;

procedure TPedidoAIQGourmet.setetdidentificacao(const Value: string);
begin
  fetdidentificacao := Value;
end;

procedure TPedidoAIQGourmet.setimmhoraimpresso(const Value: string);
begin
  fimmhoraimpresso := Value;
end;

procedure TPedidoAIQGourmet.setimmnumepedido(const Value: string);
begin
  fimmnumepedido := Value;
end;

procedure TPedidoAIQGourmet.setorcchave(const Value: integer);
begin
  forcchave := Value;
end;

{ TPedidoAIQ }

procedure TPedidoAIQ.setaiqpedido(const Value: string);
begin
  faiqpedido := Value;
end;

end.
