  object idm: TUniQuery
    SQL.Strings = (
      'SELECT '
      '  idm.idmchave,'
      '  idm.mdrchave,'
      '  etdidentificacao,'
      '  etdapelido,'
      '  eteemail,'
      '  etecontato,'
      '  idm.idmenviado'
      'FROM idm,'
      '     mdr,'
      '     etd,'
      '     ete'
      'WHERE idm.mdrchave = mdr.mdrchave'
      'AND idm.etdcodigo = etd.etdcodigo'
      'AND idm.etecodigo = ete.etecodigo'
