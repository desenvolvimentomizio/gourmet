  inherited uqtabela: TUniQuery
    SQL.Strings = (
      ' SET @CheChaveAtual = :chechaveatual;'
      ''
      ''
      'SELECT tch.tchcodigo'
      '     , tch.ctacodigo'
      '     , cta.ctaidentificacao'
      '     , tch.clbcodigo'
      '     , clb.clbidentificacao'
      '     , tch.tchfolhainicial'
      '     , tch.tchfolhafinal'
      '     , tchDisp.tchtotalfolhas'
      '     , tchDisp.cheutilizados'
      '     , tchDisp.tchfolhasdisp'
      'FROM tch'
      '  JOIN cta'
      '    ON tch.ctacodigo = cta.ctacodigo'
      '  JOIN clb'
      '    ON tch.clbcodigo = clb.clbcodigo'
      '  JOIN ctc'
      '    ON cta.ctacodigo = ctc.ctacodigo'
      '    AND ctc.clbcodigo = :clbcodigo'
      '  INNER JOIN (SELECT'
      '      tch.tchcodigo'
      
        '    , CAST((tch.tchfolhafinal - (tch.tchfolhainicial - 1)) AS UN' +
        'SIGNED) AS tchtotalfolhas'
      '    , COUNT(che.chechave) AS cheutilizados'
      
        '    , (tch.tchfolhafinal - (tch.tchfolhainicial - 1)) - COUNT(ch' +
        'e.chechave) AS tchfolhasdisp'
      '    FROM tch'
      '      LEFT JOIN che'
      '        ON tch.tchcodigo = che.tchcodigo'
      
        '        AND che.chechave <> IF(@CheChaveAtual IS NULL, -1, @CheC' +
        'haveAtual) -- Na altera'#231#227'o do registro o pr'#243'prio cheque n'#227'o pode' +
        ' constar como utilizado.'
      '    GROUP BY tch.tchcodigo) tchDisp'
      '    ON tch.tchcodigo = tchDisp.tchcodigo')
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'chechaveatual'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'clbcodigo'
        Value = nil
      end>
