  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  pcb.pcbchave,'
      '  pcb.ccochave,'
      '  pcb.pcgcodigo,'
      '  pcg.pcgestrutural,'
      '  pcg.pcgidentificacao,'
      '  pcb.phgcodigo,'
      '  phg.phgidentificacao,'
      '  pcb.pcbvalor,'
      '  pcb.pcbcomplhist'
      'FROM pcb'
      '  INNER JOIN pcg'
      '    ON pcb.pcgcodigo = pcg.pcgcodigo'
      '  INNER JOIN phg'
      '    ON pcb.phgcodigo = phg.phgcodigo'
      '    where pcb.ccochave=:ccochave')
    Left = 340
    Top = 108
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ccochave'
        Value = nil
      end>
