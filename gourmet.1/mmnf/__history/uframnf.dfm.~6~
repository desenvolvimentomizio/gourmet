  object tai: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  taialiquota'
      '  ,icmcodigo'
      'FROM tai,icm'
      
        'WHERE tai.taialiquota=icm.icmaliquotas and  ufscodigoorigem = :u' +
        'fscodigoorigem'
      'AND ufscodigodestino = :ufscodigodestino'
      'AND taifinalvalidade IS NULL')
    Left = 832
    Top = 554
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ufscodigoorigem'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'ufscodigodestino'
        Value = nil
      end>
