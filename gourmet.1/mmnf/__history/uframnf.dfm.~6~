  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT mes.meschave'
      '     , etd.etdidentificacao'
      '     , etd.etdapelido'
      '     , sde.sdeidentificacao'
      '     , tdf.tdfidentificacao'
      
        '     , if(mes.mescomplementadapor>0,concat(toe.toeidentificacao,' +
        #39' NOTA NR: '#39', (select u.mesnumero from mes u where u.meschave=me' +
        's.mescomplementadapor))  , if(mes.mescomplementanota>0, concat(t' +
        'oe.toeidentificacao,'#39' NFE COMPLEMENTAR NR: '#39', (SELECT IFNULL(m.m' +
        'esnumero,'#39'0'#39') from mes m where m.meschave=mes.mescomplementanota' +
        '))  ,  toe.toeidentificacao) ) toeidentificacao'
      '     , mes.mesemissao'
      '     , mes.mesnumero'
      '     , mes.etdcodigo'
      '     , mes.messerie'
      '     , mes.meschavenfe'
      '     , mes.mesregistro'
      '     , mes.mesvalor'
      '     , mes.mesdesconto'
      '     , mes.mestotal'
      '     , mes.temcodigo'
      '     , mes.clbcodigo'
      '     , mes.trmcodigo'
      '     , mes.mesprotocolo'
      '     , mes.sdecodigo'
      '     , mes.mesdatanfe'
      '     , toe.toeorigem'
      '     , tdf.tdfcodigo'
      '     , toe.ttecodigo'
      '     , tte.tteidentificacao'
      '     , tem.temidentificacao'
      '     , mes.mesprodutos'
      '     , mes.messervicos'
      '     , toe.ttocodigo'
      '     , mes.meschaverecla'
      '     , mes.flacodigo'
      '     , mes.mespis'
      '     , mes.mescofins'
      '     , mes.mesipi'
      '     , mes.mesicm'
      '     , mes.mesicms'
      '      '
      ''
      ''
      '     '
      'FROM mes'
      '  JOIN etd'
      '    ON mes.etdcodigo = etd.etdcodigo'
      '  JOIN sde'
      '    ON mes.sdecodigo = sde.sdecodigo'
      '  JOIN tdf'
      '    ON mes.tdfcodigo = tdf.tdfcodigo'
      '  JOIN toe'
      '    ON mes.toecodigo = toe.toecodigo'
      '  JOIN tte'
      '    ON toe.ttecodigo = tte.ttecodigo'
      '  JOIN tem'
      '    ON mes.temcodigo = tem.temcodigo'
      'WHERE toe.ttmcodigo = 0'
      'AND mes.tdfcodigo IN ('#39'00'#39', '#39'55'#39')'
      
        'AND IF(mes.sdecodigo = '#39'02'#39', NOT mes.tdfcodigo = '#39'00'#39', toe.ttmco' +
        'digo = 0)'
      'AND mes.etdcodigo <> 0'
      'AND mes.flacodigo=:flacodigo'
      ''
      '-- filtros')
    Left = 244
    Top = 184
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'flacodigo'
        Value = nil
      end>
