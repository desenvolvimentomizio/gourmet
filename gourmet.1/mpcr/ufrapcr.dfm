  inherited uqtabela: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  pcr.pcrchave,'
      '  pcr.titcodigo,'
      '  pcr.pcgcodigo,'
      '  pcg.pcgestrutural,'
      '  pcg.pcgidentificacao,'
      '  pcr.phgcodigo,'
      '  phg.phgidentificacao,'
      '  pcr.pcrvalor,'
      '  pcr.pcrcomplhist'
      'FROM pcr'
      '  INNER JOIN pcg'
      '    ON pcr.pcgcodigo = pcg.pcgcodigo'
