unit ulrdo;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, ulfrmbase, Vcl.StdCtrls, Vcl.Buttons,
  Vcl.ExtCtrls, ufrabase, ufrardo, upegabase;

type
  Tlrdo = class(Tlfrmbase)
    frardo: Tfrardo;
    procedure frardoActImprimirNFCeExecute(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  lrdo: Tlrdo;

implementation

{$R *.dfm}

function formu(pCargaFrame: TCargaFrame): string;
begin
  pCargaFrame.Titulo := vPlTitMdl;
  lrdo := Tlrdo.Create(pCargaFrame);
  try
    lrdo.frardo.TxtFiltro := pCargaFrame.Filtro;
    lrdo.frardo.Acesso := pCargaFrame.Acesso;

    // lrdo.frardo.uqtabela.Filter := pCargaFrame.Filtro;
    // lrdo.frardo.uqtabela.Filtered := true;
 //   lrdo.frardo.ActAtualizar.Execute;
    lrdo.ShowModal;
    Result := lrdo.frardo.vretorno;
  finally
    lrdo.Free;
  end;
end;

exports formu;


procedure Tlrdo.frardoActImprimirNFCeExecute(Sender: TObject);
begin
 // inherited;
  frardo.ActImprimirNFCeExecute(Sender);

end;

end.
