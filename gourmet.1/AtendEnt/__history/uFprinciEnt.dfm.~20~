  object grp: TUniQuery [53]
    Connection = Conexao
    SQL.Strings = (
      'SELECT'
      '  grp.grpcodigo,'
      '  grp.grpidentificacao,'
      '  pro.tpocodigo'
      'FROM pro'
      '  INNER JOIN grp'
      '    ON pro.grpcodigo = grp.grpcodigo and grp.sipcodigo=1'
      '  INNER JOIN gri'
      '    ON grp.grpcodigo = gri.grpcodigo'
      'WHERE pro.tpocodigo = 0'
      'and (gri.grientrega=1) and pro.sipcodigo=1'
      'GROUP BY grp.grpidentificacao'
      'ORDER BY grp.grpordem,grp.grpidentificacao')
    Left = 1177
    Top = 319
