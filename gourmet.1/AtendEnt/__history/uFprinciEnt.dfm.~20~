  inherited qpro: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  pun.puncodigo,'
      '  pro.procodigo,'
      '  pro.grpcodigo,'
      '  pro.pronome,'
      '  uni.unisimbolo,'
      '  uni.uninome,'
      '  pun.punbarra,'
      '  pun.punprecoav,'
      '  pun.punprecoap,'
      '  pro.prosaldodisp,'
      '  uni.unicodigo,'
      '  pun.punidentificacao,'
      '  pro.tpocodigo,'
      '  pun.punmultiplicador,'
      '  pro.sipcodigo,'
      '  pro.propedecomple,'
      '  pro.gracodigo,'
      '  CONVERT(COALESCE((SELECT'
      '      sfn.sfnquantidade'
      '    FROM sfn'
      '    WHERE sfn.procodigo = pro.procodigo'
      
        '    AND sfn.unicodigo = uni.unicodigo), 0), DECIMAL) AS sfnquant' +
        'idade,'
      '  IF(pro.unicodigo = pun.unicodigo, 0, 1) AS ordem,'
      '  pun.puncusto'
      'FROM pro,'
      '     pun,'
      '     uni,'
      '     grp'
      'WHERE pro.grpcodigo = grp.grpcodigo'
      
        'AND pro.procodigo = pun.procodigo and pro.sipcodigo=1 and grp.si' +
        'pcodigo=1'
      'AND pun.unicodigo = uni.unicodigo'
      
        'AND pun.tuncodigo IN (1, 9) /*tun-Tipo de Unidade (1=Entrada 9=A' +
        'mbos)*/'
      'AND pro.procodigo = :procodigo'
      'ORDER BY ordem')
    Left = 392
    Top = 216
    object qprosfnquantidade: TFloatField
      FieldName = 'sfnquantidade'
    end
