  inherited lito: TUniQuery
    SQL.Strings = (
      'SELECT'
      '  ito.itochave,'
      '  ito.itoitem,'
      '  ito.itoquantidade,'
      '  ito.procodigo,'
      '  pro.pronome,'
      '  ito.itovalorav+itoacrescimoav AS itovalor,'
      '  ito.itototalav+itoacrescimoav AS itototal,'
      '  ito.itodescontoav AS itodesconto,'
      '  ito.itosaldoav+itoacrescimoav AS itosaldo,'
      '  pro.prosaldodisp,'
      '  uni.unisimbolo,'
      '  ito.unicodigo,'
      '  ito.orcchave,'
      '  ito.tdecodigo,'
      '  pro.proreferencia,'
      '  ito.itoproservico,'
      '  ito.itoinfadprod,'
      '  pro.tpocodigo,'
      '  ito.itoprocomple,'
      '  pro.propededescrserv,'
      '  pro.propedetecnicoserv,'
      '  ito.vrpcodigo,'
      '  ito.puncodigo,'
      
        '  IF(orc.puocodigo = 0, ito.itovalorav+itoacrescimoav, ito.itova' +
        'lorap) AS itoacrescimovalor,'
      
        '  IF(orc.puocodigo = 0, ito.itototalav+itoacrescimoav, ito.itoto' +
        'talap) AS itoacrescimototal,'
      
        '  IF(orc.puocodigo = 0, ito.itosaldoav+itoacrescimoav, ito.itosa' +
        'ldoap) AS itoacrescimosaldo,'
      '  itopercentualcombo,'
      '  pcocodigo'
      'FROM ito,'
      '     pro,'
      '     uni,'
      '     orc'
      'WHERE ito.orcchave = orc.orcchave'
      'AND ito.procodigo = pro.procodigo'
      'AND ito.unicodigo = uni.unicodigo'
      'AND ito.stocodigo IN (1, 2)'
      'AND ito.orcchave = :orcchave'
      'AND ito.flacodigo = :flacodigo'
      'ORDER BY ito.itoitem')
