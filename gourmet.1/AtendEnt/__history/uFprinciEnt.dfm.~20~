  object pro: TUniQuery [54]
    Connection = Conexao
    SQL.Strings = (
      'SELECT'
      '  pro.procodigo,'
      '  pronome pronome,'
      '  procomposto'
      'FROM pro'
      'WHERE pro.tpocodigo = 0'
      'and pro.grpcodigo=:grpcodigo'
      'and pro.sipcodigo=1'
      'order by pronome'
      ''
      ''
      '/*'
      'SELECT'
      '  pro.procodigo,'
      '  pun.puncodigo,'
      
        '  concat(pro.pronome,'#39' '#39',if(upper(uni.uninome)='#39'UNIDADE'#39','#39#39',uni.' +
        'uninome)) as pronome, '
      '  pun.punprecoav,'
      '  pro.unicodigo as unipro,'
      '  pun.unicodigo as unipun'
      'FROM pun'
      '  INNER JOIN pro'
      '    ON pun.procodigo = pro.procodigo'
      '  INNER JOIN uni'
