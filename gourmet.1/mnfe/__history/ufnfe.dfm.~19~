  object itm: TUniQuery
    SQL.Strings = (
      'SELECT itm.cfocfop'
      '     , itm.procodigo'
      '     , pro.pronome'
      '     , pro.pronomereduzido'
      '     , pro.proncm'
      '     , itm.itmquantidade'
      '     , uni.unisimbolo'
      '     , itm.itmvalor'
      '     , itm.itmdesconto'
      '     , itm.itmchave'
      '     , itm.cstcodigo'
      '     , itm.icmcodigo'
      '     , itm.itmicm'
      '     , itm.itmbicm'
      '     , itm.itmbicms'
      '     , itm.itmaliqicms'
      '     , itm.itmicms'
      '     , itm.itmaliqipi'
      '     , itm.itmbipi'
      '     , itm.itmipi'
      '     , itm.itmvlribpt'
      '     , itm.itmoutras'
      '     , itm.itmdesccomple'
      
        '     , IF(ISNULL(pun.punqtdtrib),itm.itmquantidade, pun.punqtdtr' +
        'ib* itm.itmquantidade)  AS punqtdtribtotal'
      
        '     ,IF ( ISNULL(puni.unisimbolo),uni.unisimbolo,puni.unisimbol' +
        'o)                    AS unisimbolotrib'
      '     , pro.proanpcodigo'
      
        '     , ROUND(((itm.itmquantidade * itm.itmvalor) * (IFNULL(tcg.t' +
        'cgaliqnac, 0) / 100)), 2) AS itmcargatrib'
      
        '     , ROUND(((itm.itmquantidade * itm.itmvalor) * (IFNULL(tcg.t' +
        'cgaliqest, 0) / 100)), 2) AS itmcargatribest'
      '     , pro.tpocodigo'
      '     , if(pro.procest<>'#39#39',pro.procest, '#39#39') tcecest'
      '     , itm.cspcodigo'
      '     , itm.csfcodigo'
      '     , itm.csicodigo'
      '     , itm.itmtotal'
      ''
      '     , itm.itmpercreducaobaseicm'
      '     , itm.itmaliqpis'
      '     , itm.itmpis'
      '     , itm.itmbpis'
      ''
      '     , itm.itmaliqcofins'
      '     , itm.itmcofins'
      '     , itm.itmbcofins'
      '     , itm.itmfrete'
      '     , pun.punbarra'
      '     , itm.toecodigo'
      '     , itm.itmitempedido'
      
        '     , (select padextipi from pad where pad.padcodigo=pro.padcod' +
        'igo) padextipi '
      '     , itmmva'
      '     , proproducao'
      '     , uni.unicodigo'
      '     , pro.nrtcodigo    '
      '    '
      ''
      '  ,itmbasefcpicm'
      '  ,itmpercfcpicm'
      '  ,itmvalofcpicm'
      '  ,itmbasefcpicmst'
      '  ,itmpercfcpicmst'
      '  ,itmvalofcpicmst'
      '  , icmaliquotas'
      ''
      '  FROM itm'
      '  INNER JOIN pro     ON itm.procodigo  = pro.procodigo'
      '  INNER JOIN icm     ON itm.icmcodigo  = icm.icmcodigo'
      ''
      '  LEFT JOIN uni      ON itm.unicodigo  = uni.unicodigo'
      
        '  LEFT JOIN pun      ON itm.procodigo  = pun.procodigo AND itm.u' +
        'nicodigo  = pun.unicodigo'
      '  LEFT JOIN uni puni ON pun.pununitrib = puni.unicodigo'
      '  LEFT JOIN tcg      ON tcg.tcgncm     = pro.proncm'
      '  LEFT JOIN lcn      ON tcg.tcgncm     = lcn.tcgncm'
      ' WHERE  pro.tpocodigo <> 9'
      '   AND'
      '  itm.meschave = :meschave'
      '   AND itm.flacodigo= :flacodigo'
      ' GROUP BY itm.itmchave')
    Left = 204
    Top = 380
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'meschave'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'flacodigo'
        Value = nil
      end>
    object itmcfocfop: TStringField
      FieldName = 'cfocfop'
      Size = 5
    end
