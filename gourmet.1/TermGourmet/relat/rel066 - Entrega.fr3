    <TfrxUniDACQuery Name="brd" UserName="brd" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="ito" IgnoreDupParams="False" SQL.Text="-- BORDAS&#13;&#10;SELECT distinct&#13;&#10;       bri.itochave                                                                    &#13;&#10;     , brd.brdidentificacao         &#13;&#10;  FROM bri &#13;&#10; INNER JOIN brd ON bri.brdcodigo = brd.brdcodigo&#13;&#10; ORDER BY brd.brdidentificacao&#13;&#10;  " MasterFields="itochave=itochave" SpecificOptions.Text="" PropData="05704C656674026C0470546F7003BC000A506172616D65746572730100"/>
    <TfrxUniDACQuery Name="ito" UserName="ito" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SET @orcchave      =  :orcchave;  &#13;&#10;SET @immnumepedido =  :immnumepedido;  &#13;&#10;SET @tcicodigo     =  :tcicodigo;      &#13;&#10;&#13;&#10;SELECT distinct ito.itoitem        &#13;&#10;     , ito.itochave                                    &#13;&#10;     , ito.procodigo          &#13;&#10;    -- , IF(sfn.sfncodigo IS NULL,CONCAT(uni.unisimbolo,' ', pro.pronome), CONCAT(pro.pronomereduzido,' ',uni.uninome)) pronome&#13;&#10;     , pro.pronome                                &#13;&#10;     , ito.itoquantidade      &#13;&#10;     , uni.unisimbolo&#13;&#10;     , uni.uninome                                                                                  &#13;&#10;     , ito.itovalorav         &#13;&#10;     , ito.itototalav         &#13;&#10;     , itoobs&#13;&#10;     , imm.immcopos &#13;&#10;     , imm.immpratos&#13;&#10;     , grp.grpidentificacao                      &#13;&#10;  FROM ito&#13;&#10; INNER JOIN orc ON ito.orcchave = orc.orcchave&#13;&#10; INNER JOIN clb ON orc.clbcodigo = clb.clbcodigo       &#13;&#10; INNER JOIN imm ON ito.itochave = imm.itochave&#13;&#10; INNER JOIN pro ON ito.procodigo = pro.procodigo&#13;&#10; INNER JOIN grp ON grp.grpcodigo = pro.grpcodigo                                                                                                                    &#13;&#10; INNER JOIN uni ON ito.unicodigo = uni.unicodigo&#13;&#10; INNER JOIN tci ON imm.tcicodigo = tci.tcicodigo     &#13;&#10;  LEFT JOIN sfn &#13;&#10;         ON pro.procodigo = sfn.procodigo &#13;&#10;        AND uni.unicodigo = sfn.unicodigo&#13;&#10; WHERE imm.immchave  IN (SELECT immchave FROM tped where orcchave = @orcchave and tcicodigo = @tcicodigo and immnumepedido = @immnumepedido)&#13;&#10;   and ito.stocodigo not in (9, 88)                                                                             &#13;&#10; ORDER BY  tci.tciprioridade          &#13;&#10;          &#13;&#10;/* WHERE ito.orcchave      = @orcchave&#13;&#10;   AND imm.tcicodigo     = @tcicodigo       &#13;&#10;   AND immnumepedido     = @immnumepedido */                             &#13;&#10;       &#13;&#10; -- ORDER BY pronome&#13;&#10;    " SpecificOptions.Text="" PropData=""/>
    <TfrxUniDACQuery Name="orc" UserName="orc" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="CREATE TEMPORARY TABLE IF NOT EXISTS tped&#13;&#10;     ( id            INT NOT NULL AUTO_INCREMENT&#13;&#10;     , orcchave      INT&#13;&#10;     , immchave      INT&#13;&#10;     , immnumepedido INT &#13;&#10;     , tcicodigo     INT&#13;&#10;     , pedidoaux     INT&#13;&#10;     , PRIMARY KEY (id)&#13;&#10;     ) ENGINE = INNODB;    &#13;&#10;  &#13;&#10;SET @orcchave      = :orcchave;&#13;&#10;SET @immnumepedido = :immnumepedido;&#13;&#10;SET @tcicodigo     = :tcicodigo;                                                                                             &#13;&#10;SET @auxsim        = (SELECT COUNT(*) &#13;&#10;                        FROM tped &#13;&#10;                       where orcchave      = @orcchave     &#13;&#10;                         and immnumepedido = @immnumepedido&#13;&#10;                         and tcicodigo     = @tcicodigo     &#13;&#10;                         and pedidoaux = 1);&#13;&#10;&#13;&#10;SET @auxnao        = (SELECT COUNT(*) &#13;&#10;                        FROM tped &#13;&#10;                       where orcchave      = @orcchave     &#13;&#10;                         and immnumepedido = @immnumepedido&#13;&#10;                         and tcicodigo     = @tcicodigo     &#13;&#10;                         and pedidoaux = 0);&#13;&#10;&#13;&#10;SELECT distinct&#13;&#10;       orc.orcchave                                                                    &#13;&#10;     , orc.etdcodigo&#13;&#10;     , etd.etdidentificacao&#13;&#10;     , etd.etdapelido&#13;&#10;     , edr.edrrua&#13;&#10;     , edr.edrnumero&#13;&#10;     , edr.edrcomple&#13;&#10;     , edr.edrbairro&#13;&#10;     , edr.edrobs                          &#13;&#10;     , etf.etftelefone&#13;&#10;     , clb.clbidentificacao&#13;&#10;     , orc.orcdataabert&#13;&#10;     , orc.foacodigo&#13;&#10;     , orc.orcmesa&#13;&#10;     , imm.immnumepedido&#13;&#10;     , imm.immhorapedido&#13;&#10;     , imm.tcicodigo&#13;&#10;     , orc.orcobs                                  &#13;&#10;     , ifnull((SELECT SUM(ito.itototalav) FROM ito WHERE ito.orcchave = orc.orcchave AND ito.stocodigo not in (88)),0) vltotal&#13;&#10;     , @auxnao pedauxnao&#13;&#10;     , @auxsim pedauxsim                    &#13;&#10;  FROM orc&#13;&#10; INNER JOIN ito on orc.orcchave  = ito.orcchave&#13;&#10; INNER JOIN imm ON ito.itochave  = imm.itochave&#13;&#10; INNER JOIN clb ON imm.clbcodigo = clb.clbcodigo                        &#13;&#10; INNER JOIN etd ON orc.etdcodigo = etd.etdcodigo&#13;&#10; INNER JOIN edr ON edr.edrcodigo = orc.edrcodigo&#13;&#10;  LEFT JOIN etf ON etf.etdcodigo = etd.etdcodigo&#13;&#10; WHERE orc.orcchave = @orcchave&#13;&#10;   AND imm.immnumepedido = @immnumepedido&#13;&#10;   AND IF(etf.etfcodigo &#62; 0, orc.orctelefone=etf.etftelefone, 1 = 1)&#13;&#10;   AND imm.tcicodigo = @tcicodigo                                      &#13;&#10; &#13;&#10;&#13;&#10;    " SpecificOptions.Text="" PropData=""/>
    <TfrxUniDACQuery Name="sbr" UserName="sbr" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="ito" IgnoreDupParams="False" SQL.Text="/*-- SABORES&#13;&#10;&#13;&#10;SELECT distinct&#13;&#10;       sbi.itochave&#13;&#10;     , sbr.sbrcodigo                                                        &#13;&#10;     , sbr.sbridentificacao&#13;&#10;     , sbi.sbiobs&#13;&#10;  FROM sbi &#13;&#10; INNER JOIN sbr ON sbi.sbrcodigo = sbr.sbrcodigo&#13;&#10; ORDER BY sbr.sbridentificacao  */&#13;&#10;&#13;&#10;&#13;&#10;-- SABORES&#13;&#10;SELECT distinct&#13;&#10;       sbi.itochave&#13;&#10;     , sbr.sbrcodigo                                                        &#13;&#10;     , sbr.sbridentificacao&#13;&#10;     , sbi.sbiobs&#13;&#10;     , ifnull(sfn.sfncodigo,0) sfncodigo                                                                                    &#13;&#10;  FROM sbi &#13;&#10; INNER JOIN sbr ON sbi.sbrcodigo = sbr.sbrcodigo&#13;&#10; INNER JOIN (SELECT itochave itemid&#13;&#10;                  , procodigo&#13;&#10;                  , unicodigo&#13;&#10;               FROM ito) tito&#13;&#10;         ON sbi.itochave = tito.itemid &#13;&#10; INNER JOIN pro ON tito.procodigo = pro.procodigo&#13;&#10; &#13;&#10;  LEFT JOIN sfn&#13;&#10;         ON pro.procodigo = sfn.procodigo &#13;&#10;        AND tito.unicodigo = sfn.unicodigo&#13;&#10; ORDER BY sbr.sbridentificacao&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;  " MasterFields="itochave=itochave" SpecificOptions.Text="" PropData="05704C656674026C0470546F7003F0000A506172616D65746572730100"/>
    <TfrxUniDACQuery Name="isi" UserName="isi" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="sbr" IgnoreDupParams="False" SQL.Text="-- INGREDIENTE&#13;&#10;SELECT distinct&#13;&#10;       sbi.itochave&#13;&#10;     , sbi.sbrcodigo                &#13;&#10;     , isi.procodigo&#13;&#10;     , isi.isitipo&#13;&#10;     , ing.pronome ingnome&#13;&#10;     , tsi.tsicodigo                                      &#13;&#10;     , IF(tsi.tsicodigo = 3, '', tsi.tsiidentificacao) AS tsiidentificacao   &#13;&#10;  FROM sbi&#13;&#10; INNER JOIN isi ON sbi.sbichave = isi.sbichave&#13;&#10; INNER JOIN tsi ON isi.tsicodigo = tsi.tsicodigo &#13;&#10; INNER JOIN pro ing ON isi.procodigo = ing.procodigo&#13;&#10;  WHERE IF(isi.isitipo = 0, tsi.tsicodigo != 3, 1 = 1 )&#13;&#10;   ORDER BY isi.isitipo, ing.pronome " MasterFields="itochave=itochave;sbrcodigo=sbrcodigo" SpecificOptions.Text="" PropData="05704C656674026C0470546F700328010A506172616D65746572730100"/>
