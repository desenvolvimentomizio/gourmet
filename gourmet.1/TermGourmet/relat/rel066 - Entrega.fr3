<TfrxReport Version="5.2" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="42070,481905081" ReportOptions.Description.Text="" ReportOptions.LastChange="42244,7446773958" ScriptLanguage="PascalScript" ScriptText.Text=" var vppratos :integer;&#13;&#10; vpcopos : integer;                                                  &#13;&#10;   &#13;&#10;&#13;&#10;procedure GroupsaborOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  {  if &#60;sbr.&#34;sbridentificacao&#34;&#62;&#60;&#62;null then // sabor                 &#13;&#10;    begin&#13;&#10;        Groupsabor.visible :=true;              &#13;&#10;    end else&#13;&#10;    begin&#13;&#10;        Groupsabor.visible :=false;&#13;&#10;    end;}    &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure GroupTipoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   &#13;&#10;  {  if &#60;ito.&#34;isitipo&#34;&#62;=1 then      // Tipo igual a um mostra o título &#34;Adicionar&#34;                                                                                                                                   &#13;&#10;    begin&#13;&#10;        GroupTipo.visible :=true;  &#13;&#10;    end else&#13;&#10;    begin&#13;&#10;        GroupTipo.visible :=false;  &#13;&#10;    end; }            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterSaborOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; {   if &#60;ito.&#34;ingnome&#34;&#62;&#60;&#62;'' then // ingredientes do sabor                                                                                                     &#13;&#10;    begin&#13;&#10;        MasterSabor.visible :=true;              &#13;&#10;    end else&#13;&#10;    begin&#13;&#10;        MasterSabor.visible :=false;    &#13;&#10;    end; }         &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure MasterData1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     if &#60;ito.&#34;immcopos&#34;&#62; &#62; 0 then&#13;&#10;        vpcopos := vpcopos + &#60;ito.&#34;immcopos&#34;&#62;;&#13;&#10;        &#13;&#10;     if &#60;ito.&#34;immpratos&#34;&#62; &#62; 0 then&#13;&#10;        vppratos := vppratos + &#60;ito.&#34;immpratos&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    vpcopos:=0;&#13;&#10;    vppratos:=0;&#13;&#10;&#13;&#10;    if (&#60;orc.&#34;foacodigo&#34;&#62;=2) AND (&#60;orc.&#34;pedauxnao&#34;&#62; &#62; 0) then&#13;&#10;    begin&#13;&#10;      ChildValor.visible   := true;  &#13;&#10;      ChildCliente.visible := true;            &#13;&#10;    end else&#13;&#10;    begin&#13;&#10;      ChildValor.visible   :=false;  &#13;&#10;      ChildCliente.visible :=false;            &#13;&#10;    end&#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ChildObsItoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;      if &#60;ito.&#34;itoobs&#34;&#62;='' then   //Observação do produto                                                &#13;&#10;    begin&#13;&#10;        ChildObsIto.visible :=false;                                             &#13;&#10;    end else&#13;&#10;    begin&#13;&#10;        ChildObsIto.visible :=true;        &#13;&#10;    end;  &#13;&#10;     &#13;&#10;end;&#13;&#10;&#13;&#10;procedure SubdetailData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     if &#60;isi.&#34;tsicodigo&#34;&#62;=1 then&#13;&#10;      begin&#13;&#10;          Mingnome.color:=clblack;&#13;&#10;          Mingnome.font.color:=clwhite;&#13;&#10;                                                                                         &#13;&#10;        end&#13;&#10;       else&#13;&#10;      begin&#13;&#10;          Mingnome.color:=clwhite;&#13;&#10;          Mingnome.font.color:=clblack;              &#13;&#10;        end     &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ChildObsSaborOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   {  if &#60;sbr.&#34;sbiobs&#34;&#62;='' then  // Observação do sabor                                                                           &#13;&#10;    begin                        &#13;&#10;        ChildObsSabor.visible :=false;                                             &#13;&#10;    end else&#13;&#10;    begin&#13;&#10;        ChildObsSabor.visible :=true;        &#13;&#10;    end;  }     &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure DetailDataSaborOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       if &#60;sbr.&#34;sfncodigo&#34;&#62; = 0 then&#13;&#10;            DetailDataSabor.visible := False  &#13;&#10;       else&#13;&#10;            DetailDataSabor.visible := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure HeaderSaborOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     if &#60;sbr.&#34;sfncodigo&#34;&#62; = 0 then&#13;&#10;        HeaderSabor.Visible := False                                                                     &#13;&#10;     else&#13;&#10;         HeaderSabor.Visible := True;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end.">
