  inherited registro: TUniQuery
    SQL.Strings = (
      'SELECT orc.orcchave         -- chave'
      '     , orc.etdcodigo        -- entidade'
      '     , orc.clbcodigo        -- atendente'
      '     , orc.stocodigo        -- 1 Em digita'#231#227'o'
      '     , orc.orcmesa          -- mesa'
      '     , orcpessoas'
      '     , sto.stoidentificacao -- situa'#231#227'o da mesa'
      '     , orc.orcimpressao'
      '     , mor.meschave'
      '  FROM orc'
      ' INNER JOIN sto ON orc.stocodigo = sto.stocodigo '
      ' LEFT JOIN mor on orc.orcchave = mor.orcchave '
      ' WHERE orc.orcmesa   = :orcmesa'
      '   AND orc.moccodigo  IN (1,5)'
      '   AND orc.orcdataencerr IS NULL'
      '   AND orc.stocodigo in (1, 2, 5) ')
    BeforePost = registroBeforePost
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'orcmesa'
        Value = nil
      end>
    object registroorcchave: TIntegerField
      AutoGenerateValue = arAutoInc
      FieldName = 'orcchave'
    end
    object registroetdcodigo: TIntegerField
      FieldName = 'etdcodigo'
      Required = True
    end
    object registroclbcodigo: TIntegerField
      FieldName = 'clbcodigo'
    end
    object registrostocodigo: TIntegerField
      FieldName = 'stocodigo'
      Required = True
    end
    object registroorcmesa: TIntegerField
      FieldName = 'orcmesa'
    end
    object registroorcpessoas: TIntegerField
      FieldName = 'orcpessoas'
    end
    object registrostoidentificacao: TStringField
      FieldName = 'stoidentificacao'
      ReadOnly = True
      Required = True
      Size = 50
    end
    object registroorcimpressao: TIntegerField
      FieldName = 'orcimpressao'
      Required = True
    end
  end
