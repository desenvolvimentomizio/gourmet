  object gri: TUniQuery
    Connection = DmDados.Conexao
    SQL.Strings = (
      'SELECT'
      '  pro.pronome,'
      '  pun.punprecoav,'
      '  ito.itochave,'
      '  ito.orcchave,'
      '  uni.unisimbolo'
      'FROM gri'
      '  INNER JOIN pro ON gri.grpcodigo = pro.grpcodigo'
      '  INNER JOIN ito ON ito.procodigo = pro.procodigo'
      
        '  INNER JOIN pun ON pun.procodigo = ito.procodigo  AND pun.unico' +
        'digo = ito.unicodigo'
      
        '  INNER JOIN uni ON pun.unicodigo=uni.unicodigo AND (uni.tuncodi' +
        'go=9 OR uni.tuncodigo=1)'
      '  WHERE ito.orcchave=:orcchave AND gri.griimprimeauto=1')
    Left = 632
    Top = 320
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'orcchave'
        Value = nil
      end>
  end
