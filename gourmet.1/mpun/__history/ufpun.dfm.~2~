  inherited registro: TUniQuery
    SQL.Strings = (
      
        'select puncodigo,procodigo,unicodigo,punmultiplicador,punquantid' +
        'ade,unicodigobase,'
      
        'punprecoav,punprecoap,puncusto,punmargem,punidentificacao,dgrcod' +
        'igo,punbarra,tuncodigo,pununitrib,punqtdtrib,punbarrasistema,'
      
        'punmargemap,punpercacresavap,punbase, pcoprocodigo, punmaximoclb' +
        'producao from pun'
      'where '
      'puncodigo=:puncodigo  and'
      'pun.procodigo=:procodigo')
    Left = 204
    Top = 0
