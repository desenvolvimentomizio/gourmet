  object pco: TUniQuery
    SQL.Strings = (
      'SELECT '
      '   0 procodigo,'
      '   '#39'NORMAL'#39' pronome'
      ''
      ''
      'union all'
      'SELECT'
      '  procodigo,'
      '  pronome'
      'FROM pro'
      'WHERE procodigo IN (SELECT'
      '    procodigo'
      '  FROM pco)'
      'GROUP BY procodigo'
      'ORDER BY procodigo')
    Left = 468
    Top = 88
    object pcoprocodigo: TIntegerField
      FieldName = 'procodigo'
    end
    object pcopronome: TStringField
      FieldName = 'pronome'
      Size = 50
    end
  end
