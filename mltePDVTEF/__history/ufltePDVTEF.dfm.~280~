  object dtl: TUniQuery
    SQL.Strings = (
      'select '
      '   dtlchave, '
      '   ltechave, '
      '   cedcodigo, '
      '   dtl.mdacodigo, '
      '   dtlvalor, '
      '   dtlvalorinfo, '
      '   flacodigo, '
      '   ccxchave, '
      '   dtlregistro, '
      '   rdcnrauto,'
      '   mda.mdaidentificacao'
      ''
      'from dtl,mda '
      '   where dtl.mdacodigo=mda.mdacodigo'
      '   and ltechave=:ltechave')
    Left = 380
    Top = 328
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ltechave'
        Value = nil
      end>
